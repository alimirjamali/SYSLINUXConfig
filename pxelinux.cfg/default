DEFAULT vesamenu.c32
PROMPT 0
MENU TITLE PXEBoot Server. My favorite Tools & Gadgets
TIMEOUT 200
TOTALTIMEOUT 6000
ONETIMEOUT local

LABEL local
	MENU LABEL Boot existing OS
	MENU DEFAULT
	LOCALBOOT 0

LABEL Memtest
	MENU LABEL Run Memtest86+ (RAM test)
	KERNEL memtest/memtest

LABEL hdt
	MENU LABEL Hardware Detection Tool (HDT)
	COM32 hdt.c32

LABEL reboot
	MENU LABEL Reboot
	COM32 reboot.c32

LABEL poweroff
	MENU LABEL Power Off
	COM32 poweroff.c32

MENU end
